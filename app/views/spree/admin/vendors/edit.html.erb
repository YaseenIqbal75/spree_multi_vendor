<% content_for :page_title do %>
  <%= link_to plural_resource_name(Spree::Vendor), spree.admin_vendors_url %> /
  <%= @vendor.name %>
<% end %>

<%#= form_for [:admin, @vendor], html: {remote: true,format: :js},authenticity_token: true do |f| %>
<%= form_for [:admin, @vendor] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <div class="form-actions" data-hook="buttons">
      <%= button Spree.t('actions.update'), 'refresh', 'submit', {class: 'btn-success update_vendor_button', data: { disable_with: "#{ Spree.t(:saving) }..." }} %>
      <span class="or"><%= Spree.t(:or) %></span>
      <%= button_link_to Spree.t('actions.cancel'), collection_url, icon: 'delete' %>
    </div>
  </fieldset>
<% end %>
